{
  "version": 2,
  "functions": {
    "src/api/bot.ts": {
      "runtime": "node",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/commands/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/services/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/models/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/utils/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/api/bot.ts"
    }
  ]
}
